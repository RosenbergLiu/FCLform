// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Batch {
    machine_number String @id
    submit_date DateTime
    model Int
    quantity Int
    license_level Int
    comment String?
}