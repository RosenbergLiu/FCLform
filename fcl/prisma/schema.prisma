// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  binaryTargets = ["darwin-arm64", "linux-musl-arm64-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = "postgres://postgres:12345678@db:5432/postgres"
}

model Batch {
    machine_number String @id
    submit_date DateTime
    model Int
    quantity Int
    license_level Int
    comment String?
}